<div class="section-map box-middle-container row-20">
    <div class="google-map" data-coords="37.59376539999999,-122.4830551" data-zoom="12" data-skin=" "
        data-marker-pos-top="30" data-marker-pos="col-md-6-right" data-marker=""></div>
    <div class="overlaybox overlaybox-side overlaybox">
        <div class="container content">
            <div class="row">
                <div class="col-md-6 overlaybox-inner box-middle" data-anima="fade-left">
                    <div class="row">
                        <div class="col-md-6">
                            <h2>Olcan Construction</h2>
                            <p class="text-left text-s">
                                Phone us if you have a general query or would like to get a free estimate for your
                                project.
                            </p>
                            <hr class="space s" />
                            <div class="btn-group social-group btn-group-icons social-colors">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <hr class="space m visible-xs" />
                            <h2>Contact</h2>
                            <hr class="space m" />
                            <ul class="fa-ul ul-squares no-margins text-s">
                                <li>
                                    <%= common.address.street %>
                                </li>
                                <li>
                                    <%= common.address.city %>,
                                        <%= common.address.state %>
                                            <%= common.address.zip %>
                                </li>
                                <li><%= common.email %></li>
                                <li><%= common.phone.display %></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
        </div>
    </div>
</div>
